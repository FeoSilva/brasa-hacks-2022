{
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start:watch": "ts-node-dev -r tsconfig-paths/register --watch src --respawn --transpile-only src/index.ts",
    "start": "ts-node-dev -r tsconfig-paths/register src/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ],
    "exclude": []
  },
  "lint-staged": {
    "*.ts": [
      "yarn run lint"
    ]
  },
  "dependencies": {
    "agenda": "^4.2.1",
    "aws-sdk": "^2.1106.0",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "mongoose": "^5.5.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nyc": "^15.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.5.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9"
  }
}
